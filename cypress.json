{
  "testFiles": "**/*.spec.js",
  "baseUrl": "http://automationpractice.com",
  "watchForFileChanges": false,
  "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    }
}